
        <nav id="sidebar" class='hidden' >
            <!-- Close Sidebar Button -->
            <div id="dismiss">
                <i class="glyphicon glyphicon-arrow-left"></i>
            </div>

            <!-- Sidebar Header -->
            <div class="sidebar-header">
                <a href='/home'><img src='https://i.imgur.com/RSxTaJF.png' style='margin:10px'></a>
            </div>

            <!-- Sidebar Links -->
            <ul class="list-unstyled ">
                <li class=" component active"><a href="/home">Home</a></li>
                <li class="component"><a href="/about">About</a></li>
                <li class="component"><a href="/services">Services</a></li>
                <li class="component">
                    <a href="#homeSubmenu" data-toggle="collapse" aria-expanded="false">WholeSale Store</a>
                    <ul class="collapse list-unstyled" id="homeSubmenu">
                        <li class="comp"><a href="/items">Market</a></li>
                        <li class="comp"><a href="#">Instructions</a></li>
                        <li class="comp"><a href="#">Reviews</a></li>
                    </ul>
                <li class="component"><a href="#">Contact</a></li>
            </ul>
        </nav>

        <div id="content">
            <button type="button" id="sidebarCollapse" class="btn btn-info navbar-btn">
                <i class="glyphicon glyphicon-menu-hamburger"></i>
            </button>
      <!-- DONT CLOSE THIS <DIV TAG> WILL FUCK EVERYTHING UP -->

      <div id='prof-pic'>
        <img id="nav_prof_pic" src="http://i.imgur.com/IulSm39.jpg" alt="profile pic">
        <a id='prof-carrot' href="#proflink" data-toggle="collapse" aria-expanded="false"></a>
        <ul class="collapse list-unstyled" id="proflink">
          <% if User.find_by_id(session[:user_id]) %>
            <li class="prof-links"><a><%= link_to('Log Out', logout_path, method: :delete)%></a></li>
            <li class="prof-links"><a><%= link_to('Profile', user_path_url(User.find_by_id(session[:user_id])))%></a></li>
          <%else%>
            <li class="prof-links"><a href="/login">Login</a></li>
            <li class="prof-links"><a href="/signup">Sign Up</a></li>
            <%end%>
        </ul>
      </div>
  <script>
  $(document).ready(function () {
      $('#sidebarCollapse').on('click', function () {
          $('#sidebar').toggleClass("hidden")
          $('#sidebarCollapse').hide()
      });
      $('#dismiss').on('click', function () {
          $('#sidebar').toggleClass("hidden")
          $('#sidebarCollapse').show()
      });

  });
  </script>

